//.....   JOB .....
//*
//* Replace '<inprefix>' with a sane value
//* Replace '<outprefix>' with a sane value
//*
//*
//RECEIVE EXEC PGM=IKJEFT01,REGION=8M
//SYSOUT    DD SYSOUT=*
//SYSPRINT  DD SYSOUT=*
//SYSTSPRT  DD SYSOUT=*
//SYSTSIN   DD *
   PROFILE NOPREFIX
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$BZC))
     DATASET(<outprefix>.CCKD.BZLIB.C) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$BZH))
     DATASET(<outprefix>.CCKD.BZLIB.H) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$BZO))
     DATASET(<outprefix>.CCKD.BZLIB.OBJ) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$CCKDA)
     DATASET(<outprefix>.CCKD.ASM) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$CCKDL))
     DATASET(<outprefix>.CCKD.LOAD) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$CCKDO))
     DATASET(<outprefix>.CCKD.OBJ) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$ZC))
     DATASET(<outprefix>.CCKD.ZLIB.C) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$ZH))
     DATASET(<outprefix>.CCKD.ZLIB.H) VOLUME(volser) UNIT(disk)
   RECEIVE  INDATASET(<inprefix>.CCKD.JCL(X$ZO))
     DATASET(<outprefix>.CCKD.ZLIB.OBJ) VOLUME(volser) UNIT(disk)
